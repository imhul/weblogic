{
    "name": "weblogic",
    "author": "Tkachuk Zakhar",
    "license": "ISC",
    "homepage": "https://weblogic.netlify.app/",
    "repository": {
        "type": "git",
        "url": "https://github.com/imhul/weblogic"
    },
    "email": "blashirk@gmail.com",
    "version": "0.0.8",
    "scripts": {
        "format": "prettier --write --plugin-search-dir=. .",
        "go": "yarn clean && cross-env NODE_ENV=development parcel public/index.html --no-cache --dist-dir dev",
        "build": "rimraf ./dist && cross-env NODE_ENV=production parcel build public/index.html --public-url /",
        "server": "netlify dev",
        "dev": "concurrently --kill-others \"yarn go\" \"yarn server\"",
        "clean": "rimraf ./dev && rimraf ./.cache"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@babel/preset-react": "^7.18.6",
        "@fontsource/open-sans": "^4.5.14",
        "@parcel/babel-preset-env": "^2.8.3",
        "@parcel/transformer-image": "^2.8.3",
        "@parcel/transformer-sass": "2.8.3",
        "@parcel/transformer-svg-react": "^2.8.3",
        "@reduxjs/toolkit": "^1.9.3",
        "antd": "^5.4.2",
        "autoprefixer": "^10.4.14",
        "axios": "^1.3.5",
        "babel-plugin-import": "^1.13.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "buffer": "^5.5.0",
        "concurrently": "^8.0.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "node-fetch": "^3.3.1",
        "parcel": "^2.8.3",
        "parcel-transformer-url-loader": "^2.0.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "react": "^18.2.0",
        "react-clipboard.js": "^2.0.16",
        "react-contextmenu": "^2.14.0",
        "react-dom": "^18.2.0",
        "react-gravatar": "^2.6.3",
        "react-helmet": "^6.1.0",
        "react-move": "^6.5.0",
        "react-recaptcha": "^2.3.10",
        "react-redux": "^8.0.5",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "rimraf": "^5.0.0",
        "sass": "^1.62.0"
    },
    "parcel-transformer-url-loader": {
        "limit": 99999
    },
    "browserslist": "> 0.5%, last 2 versions, not dead"
}
