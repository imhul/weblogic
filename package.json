{
    "name": "weblogic",
    "author": "Tkachuk Zakhar",
    "license": "ISC",
    "homepage": "https://weblogic.netlify.app/",
    "repository": {
        "type": "git",
        "url": "https://github.com/imhul/weblogic"
    },
    "email": "blashirk@gmail.com",
    "version": "1.3.2",
    "scripts": {
        "format": "prettier --write --plugin-search-dir=. .",
        "go": "yarn clean && cross-env NODE_ENV=development parcel public/index.html --no-cache --dist-dir dev",
        "build": "rimraf ./dist && cross-env NODE_ENV=production parcel build public/index.html --public-url /",
        "build-local": "netlify functions:build --functions dev/functions --src functions",
        "go-local": "netlify functions:serve --port 3001",
        "server": "netlify dev --functions functions --functions-port 3001 --live",
        "dev": "concurrently --kill-others \"yarn go\" \"yarn server\"",
        "clean": "rimraf ./dev && rimraf ./.cache",
        "lint": "stylelint \"src/**/*.scss\" --fix"
    },
    "dependencies": {
        "@ant-design/icons": "^5.1.4",
        "@babel/plugin-proposal-class-properties": "^7.28.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/preset-react": "^7.28.5",
        "@fontsource/open-sans": "^5.0.1",
        "@netlify/functions": "^1.6.0",
        "@netlify/functions-utils": "^5.2.8",
        "@parcel/babel-preset-env": "^2.16.1",
        "@parcel/optimizer-cssnano": "^2.16.1",
        "@parcel/transformer-image": "^2.16.1",
        "@parcel/transformer-sass": "2.16.1",
        "@parcel/transformer-svg-react": "^2.16.1",
        "@reduxjs/toolkit": "^1.9.5",
        "antd": "^5.5.2",
        "autoprefixer": "^10.4.14",
        "axios": "^1.4.0",
        "babel-plugin-import": "^1.13.8",
        "bowser": "^2.11.0",
        "buffer": "^6.0.3",
        "concurrently": "^8.0.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.1.0",
        "fuse.js": "^6.6.2",
        "github-markdown-css": "^5.8.1",
        "js-cookie": "^3.0.5",
        "jsonwebtoken": "^9.0.0",
        "mongodb": "^5.6.0",
        "ms": "^2.1.3",
        "nodemailer": "^6.9.3",
        "openai": "^3.2.1",
        "parcel": "^2.16.1",
        "parcel-transformer-url-loader": "^2.0.0",
        "postcss": "^8.5.6",
        "prettier": "^2.8.8",
        "process": "^0.11.10",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-clipboard.js": "^2.0.16",
        "react-contextmenu": "^2.14.0",
        "react-dom": "^18.2.0",
        "react-gravatar": "^2.6.3",
        "react-helmet": "^6.1.0",
        "react-markdown": "^10.1.0",
        "react-move": "^6.5.0",
        "react-recaptcha": "^2.3.10",
        "react-redux": "^8.0.5",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1",
        "rimraf": "^5.0.1",
        "sass": "^1.62.1",
        "stylelint": "^15.6.2",
        "stylelint-config-standard": "^33.0.0",
        "stylelint-config-standard-scss": "^9.0.0",
        "stylelint-scss": "^5.0.0",
        "undici": "^5.22.1",
        "uuid": "^9.0.0"
    },
    "parcel-transformer-url-loader": {
        "limit": 99999
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "@parcel/resolver-default": {
        "packageExports": true
    },
    "engines": {
        "parcel": "^2.16.1"
    },
    "devDependencies": {
        "crypto-browserify": "^3.12.0",
        "events": "^3.1.0",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.3"
    }
}
